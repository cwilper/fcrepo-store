<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-lazy-init="true">
  <bean id="exclude-builtin-objects"
      class="com.github.cwilper.fcrepo.store.util.filters.ExcludeByPid">
    <constructor-arg>
      <bean class="com.github.cwilper.fcrepo.store.util.PIDSpec">
        <constructor-arg value="fedora-system:ContentModel-3.0,fedora-system:FedoraObject-3.0,fedora-system:ServiceDefinition-3.0,fedora-system:ServiceDeployment-3.0"/>
      </bean>
    </constructor-arg>
  </bean>
</beans>
